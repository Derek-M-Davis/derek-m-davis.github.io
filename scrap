let https = require('https');

let request = https.request(
  {
    method: 'GET',
    hostname: 'api-v3.igdb.com',
    path: '/games/',
    headers: {
      
        'user-key': 'e9e6341303be555f38f64c89b5928eaf',
  }
}, (res) =>{
  let body = '';
  
  
  res.on('data', (data) => body+= data);
  res.on('end', () => console.log( 'response ended','Here is the data', body));
});

request.end();
